{% extends 'base.html.twig' %}

{% block title %}Activity{% endblock %}

{% block body %}
    <h1>Activity</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ activity.id }}</td>
            </tr>
            <tr>
                <th>DateAt</th>
                <td>{{ activity.dateAt ? activity.dateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_activity_index') }}">back to list</a>

    <a href="{{ path('app_admin_activity_edit', {'id': activity.id}) }}">edit</a>

    {{ include('admin/activity/_delete_form.html.twig') }}
{% endblock %}
